{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"lib": [
			"ESNext",
			"DOM",
			"DOM.Iterable"
		],
		"noEmit": true,
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"useUnknownInCatchVariables": false
	}
}
