{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"lib": ["ESNext"],
		"noEmit": true,
		"declaration": true,
		"sourceMap": true,
		"strict": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"allowSyntheticDefaultImports": true
	}
}
